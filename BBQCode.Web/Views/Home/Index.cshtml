@section slider{
    <div id="slider">
        <div id="slider_contain" class="center_margin">
            <ul>
                <li>
                    <div id="website">
                        <h2>@Html.LocalizedTitle("SliderWebsite")</h2>
                    </div>
                </li>
                <li>
                    <div id="coop">
